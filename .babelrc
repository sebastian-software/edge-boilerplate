{
  "env": {
    "edge-development-client": {
      "presets": [
        [ "edge", { "target": "modern" }]
      ]
    },

    "edge-development-server": {
      "presets": [
        [ "edge", { "target": "current" }]
      ]
    },

    "edge-production-client": {
      "presets": [
        [ "edge", { "target": "browser" }]
      ]
    },

    "edge-production-server": {
      "presets": [
        [ "edge", { "target": "script" }]
      ]
    },

    "development": {
      "presets": [
        [ "edge", { "target": "current", "modules": "commonjs" }]
      ]
    },

    "test": {
      "presets": [
        [ "edge", { "target": "test" }]
      ]
    }
  }
}
